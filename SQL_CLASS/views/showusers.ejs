<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Data</title>
    <style>
        table,
        tr,
        td {
            border: 1px solid rgb(7, 2, 2);
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Username</th>
        </tr>
        <form method="GET" action="/user/new">
            <button>Create New</button>
        </form>

       

  <%    for(user of users) { %>
        <tr>
            <th> <%= user.id %> </th>
            <th> <%= user.email %> </th>
            <th> <%= user.username %> </th> 
            <td>
            <form method="get" action="/user/<%= user.id %>/edit">
                  <button>Eidt username</button>
            </form>
        </td>
        <td>
            <form method="get" action="/user/<%=user.id%>/delete">
                <button>Delete</button>
              </form>
        </td>
        </tr>
    <%  } %>
    </table>
</body>
</html>